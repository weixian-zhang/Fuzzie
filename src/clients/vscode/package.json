{
  "name": "wxz-fuzzie",
  "displayName": "Fuzzie",
  "description": "Fuzzie is a simple grey-box fuzz testing tool targetting REST API and GraphQL",
  "publisher": "wxz",
  "author": "Weixian Zhang",
  "repository": {
    "type": "git",
    "url": "https://github.com/weixian-zhang/Fuzzie.git"
  },
  "bugs": {
    "url": "https://github.com/weixian-zhang/Fuzzie/issues"
  },
  "homepage": "https://github.com/weixian-zhang/Fuzzie",
  "license": "MIT",
  "icon": "dist/fuzzie-logo-2.png",
  "version": "0.14.0",
  "preview": true,
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other",
    "Testing"
  ],
  "keywords": [
    "fuzzing",
    "fuzzer",
    "testing",
    "fuzz",
    "fuzz test",
    "security",
    "REST",
    "rest",
    "GraphQL",
    "graphql",
    "API",
    "pentest"
  ],
  "galleryBanner": {
    "color": "#00FFFF",
    "theme": "light"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
    "*",
    "onCommand:fuzzie.openwebview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "fuzzie.openwebview",
        "title": "Fuzzie: Open"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.12",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/applicationinsights": "^0.20.0",
    "@types/axios": "^0.14.0",
    "@types/bootstrap": "^5.2.6",
    "@types/decompress": "^4.2.4",
    "@types/jszip": "^3.4.1",
    "@types/set-interval-async": "^1.0.0",
    "@types/unzip": "^0.1.1",
    "@types/ws": "^8.5.3",
    "applicationinsights": "^2.4.1",
    "axios": "^1.2.1",
    "decompress": "^4.2.1",
    "https": "^1.0.0",
    "jszip": "^3.10.1",
    "node-fetch": "^3.3.0",
    "set-interval-async": "^3.0.3",
    "unzip": "^0.1.11",
    "unzip-stream": "^0.3.1",
    "ws": "^8.10.0"
  }
}
